RewriteEngine On
RewriteBase /

# Redirect URL yang mengandung "/php/" ke URL tanpa "/php/"
RewriteCond %{THE_REQUEST} /php/([^\s?]*) [NC]
RewriteRule ^ %1 [R=301,L]

# Atur internal redirect ke "/php/"
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ php/$1 [L]
